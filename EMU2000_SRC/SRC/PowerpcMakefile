SUBDIRS = librtdb share Protocol/SocketFtp procman rdbms BayLayer Protocol/ModBus Protocol/IEC104 Protocol/UPS Protocol/IEC103  Protocol/DDB Protocol/Dlt645 Protocol/ModBusMaster Protocol/IEC101S Protocol/ModbusTcp Protocol/ModBusSlave Protocol/DataTransS Protocol/ESDCMMI cgi Protocol/CDT Protocol/LFP Protocol/KunMing
all:
	 @list='$(SUBDIRS)'; for subdir in $$list; do \
                echo "Making all in $$list"; \
                (cd $$subdir && make all); \
        done;
.PHONY: clean
clean:
	@list='$(SUBDIRS)'; for subdir in $$list; do \
                echo "Clean in $$subdir"; \
                (cd $$subdir && make clean); \
        done
distclean:
	@list='$(SUBDIRS)'; for subdir in $$list; do \
                echo "Clean in $$subdir"; \
                (cd $$subdir && make distclean); \
        done
